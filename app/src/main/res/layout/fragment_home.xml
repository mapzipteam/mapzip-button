<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <TextView
        android:id="@+id/topstate"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:background="#fafab1"
        android:paddingBottom="10dp"
        android:paddingLeft="5dp"
        android:paddingTop="10dp"
        android:text="사용자(ididid)"
        android:textAppearance="?android:attr/textAppearanceLarge" />

    <Button
        android:id="@+id/mapsetting"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:text="지도설정"
        android:background="@color/transparent"/>

    <TextView
        android:id="@+id/mapstate"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/topstate"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="5dp"
        android:text="지도 선택: "
        android:textSize="15dp"/>

    <Spinner
        android:id="@+id/spinner"
        android:layout_below="@+id/topstate"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingTop="5dp"
        android:paddingBottom="5dp"
        android:layout_marginRight="5dp"
        android:layout_marginTop="5dp"
        android:layout_toRightOf="@+id/mapstate" />

    <ImageView
        android:id="@+id/mapimage"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_above="@+id/tagText"
        android:layout_below="@+id/spinner" />

    <TextView
        android:id="@+id/tagText"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/mainOrange"
        android:gravity="center"
        android:paddingBottom="5dp"
        android:paddingTop="5dp"
        android:paddingLeft="10dp"
        android:paddingRight="10dp"
        android:text="#해쉬#태그#다섯\n#개다#그렇다"
        android:textSize="15dp"
        android:singleLine="false"
        android:layout_marginBottom="140px"
        android:layout_alignParentBottom="true"
        android:layout_alignRight="@+id/spinner"
        android:layout_alignEnd="@+id/spinner" />


        <Button
